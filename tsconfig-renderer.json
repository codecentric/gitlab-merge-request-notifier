{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "esModuleInterop": true,
    "allowJs": false,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist/renderer",
    "strict": true,
    "typeRoots": [
      "./src/renderer/types",
      "./node_modules/@types"
    ]
  },
  "include": [
    "src/renderer/*"
  ]
}
